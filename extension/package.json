{
    "name": "twitter-reply-extension",
    "version": "1.0.0",
    "description": "A browser extension to generate human-like Twitter replies using GPT-J-6B",
    "scripts": {
        "build:popup": "vite build --config vite.config.popup.js",
        "build:content": "vite build --config vite.config.content.js",
        "build": "npm run build:popup && npm run build:content",
        "dev": "vite"
    },
    "dependencies": {
        "@chakra-ui/react": "^2.10.8",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "firebase": "^11.6.0",
        "framer-motion": "^10.16.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.5.0",
        "@mantine/core": "^7.6.1",
        "@mantine/hooks": "^7.6.1",
        "@mantine/notifications": "^7.6.1",
        "@tabler/icons-react": "^2.47.0"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.0.0",
        "vite": "^4.0.0",
        "vite-plugin-html": "^3.2.2"
    }
}
